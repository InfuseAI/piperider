{
  "name": "@infuseai/piperider-ui",
  "author": {
    "name": "infuseai"
  },
  "homepage": "./",
  "description": "A UI library for building Piperider generated reports",
  "version": "0.1.0-alpha.1",
  "license": "MIT",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "@amplitude/analytics-browser": "^1.6.3",
    "@amplitude/analytics-types": "^0.13.0",
    "@chakra-ui/icons": "^2.0.12",
    "@chakra-ui/react": "^2.4.1",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@sgratzl/chartjs-chart-boxplot": "^4.1.0",
    "@tanstack/react-table": "^8.7.0",
    "chart.js": "^4.0.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "cytoscape": "git://github.com/dbt-labs/cytoscape.js#feature/cubic-bezier-edges",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.5.0",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.7",
    "framer-motion": "^7.6.12",
    "inter-ui": "^3.19.3",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.1.0",
    "react-cytoscapejs": "^2.0.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.6.0",
    "react-use": "^17.4.0",
    "zod": "^3.19.1",
    "zustand": "^4.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@craco/craco": "^7.0.0",
    "@sentry/browser": "^7.8.0",
    "@sentry/tracing": "^7.8.0",
    "@types/jest": "^28.1.2",
    "@types/lodash": "^4.14.182",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-cytoscapejs": "^1.2.2",
    "@types/react-dom": "^18.0.5",
    "chalk": "^5.0.1",
    "craco-esbuild": "^0.5.1",
    "cypress": "^10.3.0",
    "cytoscape": "git://github.com/dbt-labs/cytoscape.js#feature/cubic-bezier-edges",
    "cytoscape-cose-bilkent": "^4.1.0",
    "cytoscape-dagre": "^2.5.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "http-server": "^14.1.1",
    "json-schema-to-typescript": "^10.1.5",
    "node-html-parser": "^5.3.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react-cytoscapejs": "^2.0.0",
    "react-scripts": "5.0.1",
    "rimraf": "^3.0.2",
    "ts-to-zod": "^1.11.0",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "wouter": "^2.9.0",
    "yaml": "^2.1.3"
  },
  "scripts": {
    "analyze": "npx cra-bundle-analyzer",
    "extract:metadata": "node src/sdlc/extract-schema-metadata",
    "setup": "run-s typing:dts typing:dbt extract:metadata embed:html lint:fix",
    "serve:single": "dotenv -e ./build-env/.env.single craco start",
    "serve:comparison": "dotenv -e ./build-env/.env.comparison craco start",
    "start:single": "run-s setup serve:single",
    "start:comparison": "run-s setup serve:comparison",
    "typing:dbt": "npx json2ts schema/dbt-manifest-schema.json > src/sdlc/dbt-manifest-schema.ts",
    "typing:dts": "npx json2ts ../piperider_cli/profiler/schema.json > src/sdlc/single-report-schema.ts",
    "typing:zts": "npx ts-to-zod src/sdlc/single-report-schema.ts src/sdlc/single-report-schema.z.ts",
    "embed:html": "node src/sdlc/embed-window-data",
    "strip:html": "node src/sdlc/clean-window-data",
    "rm:cli-reports": "rimraf ../piperider_cli/data/report/*",
    "build": "run-s strip:html rm:cli-reports && run-p -l build:single build:comparison",
    "build:single": "pnpm run bundle:norm:single && mv build/single-report ../piperider_cli/data/report/single-report",
    "build:comparison": "pnpm run bundle:norm:comparison && mv build/comparison-report ../piperider_cli/data/report/comparison-report",
    "bundle:norm:single": "dotenv -e ./build-env/.env.single craco build",
    "bundle:case:single-edge": "dotenv -e ./build-env/.env.single-edge craco build",
    "bundle:norm:comparison": "dotenv -e ./build-env/.env.comparison craco build",
    "bundle:case:comparison-asymmetric": "dotenv -e ./build-env/.env.comparison-asymmetric craco build",
    "bundle:e2e:single": "dotenv -e ./build-env/.env.single-e2e craco build",
    "bundle:e2e:comparison": "dotenv -e ./build-env/.env.comparison-e2e craco build",
    "bundle:e2e": "run-s -l bundle:e2e:*",
    "bundle:e2e:case": "run-s -l bundle:case:*",
    "serve:e2e:norm:single": "http-server ./build/single-report -p 3000 --silent",
    "serve:e2e:norm:comparison": "http-server ./build/comparison-report -p 3001 --silent",
    "serve:e2e:case:single-edge": "http-server ./build/single-edge-report -p 4000 --silent",
    "serve:e2e:case:comparison-asymmetric": "http-server ./build/comparison-asymmetric-report -p 4001 --silent",
    "link:lib": "cp ./package.json dist && cd dist && pnpm link && cd ..",
    "lint": "eslint src/",
    "lint:fix": "pnpm run lint --fix"
  },
  "resolutions": {
    "nth-check": "2.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "plugin:prettier/recommended"
    ]
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "react-graph-vis": "^1.0.7",
    "vis-network": "^9.1.6"
  }
}
