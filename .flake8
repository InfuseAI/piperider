[flake8]
ignore = W605, F401, E501
max-line-length = 120
exclude = tests,
          docs,
          .git,
          .venv,
          venv,
          debug,
          build,
          dist,
          images,
          static_report,
          *.egg-info,
          piperider,
          piperider_cli/data/piperider-init-template/plugins/customized_assertions.py,
          piperider_cli/datasource/piperider_extracted
